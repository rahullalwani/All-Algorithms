#include<bits/stdc++.h>
using namespace std;


int linear_search(int a[],int n,int num)
{
    int index=-1;

    for(int i=0;i<n;i++)
    {
        if(a[i]==num)
        {
            index=i;
            break;
        }

    }
    return index;

}



int main()
{
    int n;
    int a[10];
    int num;
    cin>>n;
    for(int i=0;i<n;i++)
        cin>>a[i];
    cin>>num;
    int index=linear_search(a,n,num);
    cout<<index<<endl;

    return 0;
}
